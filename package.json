{
  "name": "prettier-package-json",
  "description": "Prettier formatter for package.json files",
  "license": "MIT",
  "author": "Cameron Hunter <hello@cameronhunter.co.uk>",
  "repository": {
    "type": "git",
    "url": "https://github.com/cameronhunter/prettier-package-json"
  },
  "bugs": "https://github.com/cameronhunter/prettier-package-json/issues",
  "version": "2.1.3",
  "main": "src/index.js",
  "bin": {
    "prettier-package-json": "./bin/prettier-package-json"
  },
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "test": "jest",
    "preversion": "npm test",
    "version": "git push && git push --tags"
  },
  "dependencies": {
    "commander": "^4.0.1",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.6",
    "minimatch": "^3.0.4",
    "parse-author": "^2.0.0",
    "sort-object-keys": "^1.1.3",
    "sort-order": "^1.0.1"
  },
  "devDependencies": {
    "cross-spawn": "^7.0.3",
    "husky": "^6.0.0",
    "jest": "^26.6.3"
  },
  "keywords": [
    "cleanup",
    "formatter",
    "json",
    "keys",
    "object",
    "package",
    "package.json",
    "prettier",
    "sort"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "./bin/prettier-package-json --write && git add package.json"
    }
  }
}
